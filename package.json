{
	"name": "effect-attio",
	"version": "0.2.1",
	"type": "module",
	"license": "MIT",
	"description": "Attio API SDK for Effect",
	"author": "Tobias MÃ¶ritz <tobias@moeritz.io>",
	"homepage": "https://github.com/tobimori/effect-attio",
	"repository": {
		"type": "git",
		"url": "https://github.com/tobimori/effect-attio.git"
	},
	"bugs": {
		"url": "https://github.com/tobimori/effect-attio/issues"
	},
	"keywords": [
		"effect",
		"attio",
		"api",
		"sdk",
		"typescript"
	],
	"publishConfig": {
		"access": "public",
		"directory": "dist"
	},
	"exports": {
		"./package.json": "./package.json",
		".": "./src/index.ts",
		"./*": "./src/*.ts"
	},
	"scripts": {
		"build": "pnpm clean && tsc -b tsconfig.build.json && cp -r src dist/src && cp package.json README.md LICENSE dist/",
		"check": "tsc -b tsconfig.json",
		"clean": "rm -rf dist .tsbuildinfo",
		"release": "pnpm build && pnpm publish"
	},
	"peerDependencies": {
		"@effect/platform": "^0.90.2",
		"effect": "^3.17.7"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@effect/language-service": "^0.35.2",
		"@effect/platform-browser": "^0.70.0",
		"@effect/platform-bun": "^0.78.0",
		"typescript": "^5.9.2"
	}
}
